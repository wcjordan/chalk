{
  "cli": {
    "version": ">= 11.0.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "UI_ENVIRONMENT": "dev",
        "DEBUG": "true"
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "UI_ENVIRONMENT": "prod",
        "DEBUG": "false"
      }
    },
    "production": {
      "env": {
        "UI_ENVIRONMENT": "prod",
        "DEBUG": "false"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
