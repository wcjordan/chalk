{
  "cli": {
    "version": ">= 2.6.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "ENVIRONMENT": "dev",
        "DEBUG": "true"
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "ENVIRONMENT": "prod",
        "DEBUG": "false"
      }
    },
    "production": {
      "env": {
        "ENVIRONMENT": "prod",
        "DEBUG": "false"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
